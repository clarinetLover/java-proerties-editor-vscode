{
  "name": "my-extension",
  "displayName": "My Extension",
  "description": "A minimal VS Code extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "properties",
        "aliases": ["Properties", "properties"],
        "extensions": [".properties"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "properties",
        "scopeName": "source.properties",
        "path": "./syntaxes/properties.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}